esome/react-fontawesome'\nimport { faVolumeUp, faStopCircle } from '@fortawesome/free-solid-svg-icons'\n\nexport const Audio = ({url, handleAudio, id, idPlay}) => {\n\n    const [valPlay, setValPlay] = useState(true);\n    const [icon, setIcon] = useState(faVolumeUp);\n    const sondRef = useRef();\n\n    const actionAudio = () => {\n       if(valPlay === true){\n        setIcon(faStopCircle);\n        sondRef.current.play();\n        handleAudio(id);\n       }else{\n        setIcon(faVolumeUp);\n        sondRef.current.pause();\n       }\n        setValPlay(!valPlay);\n    }\n\n    return (\n        <div className=\"btoAudio\">\n            <Button variant=\"primary\" onClick={() =>  actionAudio() }>\n                <FontAwesomeIcon icon={icon}/> Escucha con atencin\n                <audio src={url} ref={sondRef} /> \n            </Button>\n        </div>                \n    )\n}\n","import React, {useState } from 'react'\nimport {Row} from 'react-bootstrap'\nimport {Audio} from './Audio'\n\nexport const Historieta = ({params}) => {\n\n    const [valAudioPlay, setAudioPlay] = useState(0);\n    const handleAudio = (id, setValPlay, setIcon, sondRef, valPlay) => {\n        setAudioPlay(id);\n    }\n\n    return (\n        <Row>\n            {\n                params.Vinetas.map(vineta => (\n                    <div key={vineta.id} className='historieta'>\n                        {   \n                            (vineta.Audios !== null)\n                            && <Audio url={vineta.Audios.url} handleAudio={handleAudio} id={vineta.id} idPlay={valAudioPlay}/>\n                        }\n                        <img src={vineta.Imagen.url} alt={vineta.Titulo}/>\n                    </div>\n                ))\n            }\n        </Row>\n    )\n}\n","import React from 'react';\nimport YouTube from 'react-youtube';\n\nclass Video extends React.Component {\n\n    constructor(props) {\n        super(props);\n    }\n\n  render() {\n    const opts = {\n      playerVa